<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" 
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Style TargetType="TextBox" x:Key="UrlTextBoxStyle">
        <Setter Property="Margin" Value="400"/>
    </Style>

    <Style TargetType="Button" x:Key="GoButtonStyle">
        <Setter Property="Margin" Value="10,0,0,0"/>
    </Style>

    <Style TargetType="Button" x:Key="AddBookmarkButtonStyle">
        <Setter Property="Margin" Value="10,10,0,0"/>
    </Style>

    <Style TargetType="WebView" x:Key="BrowserWebViewStyle">
        <Setter Property="Grid.Row" Value="2"/>
    </Style>
</ResourceDictionary>
